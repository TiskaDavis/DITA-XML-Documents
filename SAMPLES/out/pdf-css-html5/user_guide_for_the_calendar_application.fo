                                                <fo:block>
                                                  <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="1em"><fo:block end-indent="0" start-indent="0"><fo:inline chfox:inch="true" max-width="100%" id="unique_4_Connect_42_image_e3b_xtv_zdc" chfox:nd-id="image_e3b_xtv_zdc"><fo:inline><fo:external-graphic src="file:/C:/Users/tishk/Dropbox/PC%20(3)/Desktop/SAMPLES/Images/Picture6.png" max-width="100%" scaling="uniform" width="497in div 96" content-width="scale-to-fit" height="360in div 96" content-height="scale-to-fit"/></fo:inline></fo:inline></fo:block></fo:block-container>
